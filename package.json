{
  "name": "netlify-cms-toolkit",
  "version": "0.0.1-rc15",
  "private": false,
  "main": "build/cli.js",
  "bin": "build/cli.js",
  "scripts": {
    "clean": "rm -rf build",
    "netlify:proxy": "PORT=3001 netlify-cms-proxy-server",
    "netlify:admin": "PORT=3000 http-server",
    "netlify": "concurrently 'npm run netlify:proxy' 'npm run netlify:admin'",
    "build:babel": "npm run clean && babel src --out-dir build --extensions '.ts,.tsx' --source-maps && chmod u+x build/cli.js",
    "build:types": "tsc -p . --emitDeclarationOnly",
    "build:helpers": "npm run cli -- inject-helpers --file public/admin/index.html",
    "build": "npm run build:babel && npm run build:types && npm run build:helpers",
    "cli": "NODE_OPTIONS='--enable-source-maps' node build/cli.js",
    "test": "jest",
    "test:create-fixtures": "CREATE_FIXTURES=1 npm run test"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "17.0.18",
    "@types/proper-lockfile": "^4.1.2",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^6.5.1",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "http-server": "^14.1.0",
    "jest": "^27.5.1",
    "netlify-cms-app": "^2.15.66",
    "netlify-cms-core": "^2.54.5",
    "netlify-cms-proxy-server": "^1.3.23",
    "react": "^16.14.0",
    "tsd": "^0.19.1",
    "typescript": "^4.5.5",
    "uuid": "^3.4.0"
  },
  "dependencies": {
    "@types/globby": "^9.1.0",
    "change-case": "^4.1.2",
    "cheerio": "*",
    "chokidar": "^3.5.3",
    "eslint": "^7.32.0",
    "globby": "^11.0.4",
    "gray-matter": "^4.0.3",
    "js-yaml": "^3.14.1",
    "pretty-ms": "^7.0.1",
    "prettier": "^2.5.1",
    "proper-lockfile": "^4.1.2",
    "ts-lib": "^0.0.5",
    "ts-morph": "^13.0.3",
    "yargs": "^17.3.1",
    "zod": "^3.11.6"
  }
}