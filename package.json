{
  "name": "netlify-cms-toolkit",
  "version": "0.0.0-rc2",
  "private": false,
  "main": "build/cli.js",
  "bin": "build/cli.js",
  "exports": {
    "./lib": "./build/lib/lib.js",
    "./runtime": "./build/lib/runtime.js"
  },
  "typesVersions": {
    "*": {
      "./lib": [
        "./build/lib/lib.d.ts"
      ],
      "./runtime": [
        "./build/lib/runtime.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf build",
    "netlify:proxy": "PORT=3001 netlify-cms-proxy-server",
    "netlify:admin": "PORT=3000 http-server",
    "netlify": "concurrently 'npm run netlify:proxy' 'npm run netlify:admin'",
    "build:babel": "npm run clean && babel src --out-dir build --extensions '.ts' --source-maps && chmod u+x build/cli.js",
    "build:types": "tsc -p . --emitDeclarationOnly",
    "build": "npm run build:babel && npm run build:types",
    "cli": "node build/cli.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/eslint": "^8.2.1",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "17.0.0",
    "@types/yargs": "^16.0.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.5.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.0.0",
    "jest": "^27.4.5",
    "netlify-cms-proxy-server": "^1.3.22",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@types/globby": "^9.1.0",
    "chokidar": "^3.5.2",
    "eslint": "^7.32.0",
    "globby": "^11.0.4",
    "gray-matter": "^4.0.3",
    "js-yaml": "^3.14.1",
    "ts-lib": "^0.0.5",
    "ts-morph": "^13.0.2",
    "yargs": "^16.2.0",
    "zod": "^3.11.6"
  }
}